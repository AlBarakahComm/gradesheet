<h1>New campus</h1>

<%= error_messages_for :campus %>

<% form_for(@campus) do |f| %>
  <p>
    <b>School</b><br />
    <%= f.text_field :school_id %>
  </p>

  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', campuses_path %>
