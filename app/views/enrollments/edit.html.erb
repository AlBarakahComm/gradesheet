<h1>Editing enrollment</h1>

<%= error_messages_for :enrollment %>

<% form_for(@enrollment) do |f| %>
  <p>
    <b>Student</b><br />
    <%= f.text_field :student_id %>
  </p>

  <p>
    <b>Course</b><br />
    <%= f.text_field :course_id %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @enrollment %> |
<%= link_to 'Back', enrollments_path %>
