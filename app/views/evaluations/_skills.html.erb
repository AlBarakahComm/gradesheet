<form id="skill_grid">
  <table class='master fixed-grid fixed-grid-header'>
    <thead><%= skills_header %></thead>
  </table>

  <div style="height: 500px; overflow:auto;">
    <table class="fixed-grid master">
      <thead>
      </thead>
      <tbody>
        <%= skills_body %>
      </tbody>
    </table>
  </div>
</form>

<span class="paginate">
  <%= will_paginate @ctskills,
    :name           => 'skills',
    :separator      => '&nbsp;&nbsp;' %>
</span>

<% javascript_tag do %>
  // Set up the keyboard controls for this form
  <%= render :partial => 'form_control.js' %>
<% end %>
